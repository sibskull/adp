<?xml version="1.0" encoding="utf-8" ?>

<policy_template>
	<id>machine.script.execute</id>
	<name>Execute arbitrary script for machine</name>
	<name xml:lang="ru">Выполнить произвольный скрипт для компьютера</name>
	<version>0.1</version>
	<class>machine</class>
	<script>machine.script.execute.sh</script>
	<parameters>
		<param type="string" name="script" default="">
			<label xml:lang="ru">Путь к скрипту</label>
		</param>
	</parameters>
</policy_template>
