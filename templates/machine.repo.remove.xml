<?xml version="1.0" encoding="utf-8" ?>

<policy_template>
	<id>machine.repo.remove</id>
	<name>Remove source to list of active repositories</name>
	<name xml:lang="ru">Удалить источник из списка активных репозиториев</name>
	<version>0.1</version>
	<class>machine</class>
	<requires type="package">apt-repo</requires>
	<script>machine.repo.remove.sh</script>
	<parameters>
		<param type="string" name="repo" default="">
			<label xml:lang="ru">Репозиторий</label>
		</param>
	</parameters>
</policy_template>
